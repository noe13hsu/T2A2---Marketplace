<% products.each do |product| %>
    <div class="swiper-slide">
        <img src="https://via.placeholder.com/110x130">
        <div class="product-details">
            <p><%= link_to product.name, product_path(product) %></p>
            <p><%= product.console %></p>
            <p>$<%= product.price %></p>
        </div>
    </div>
<% end %>